{
  "name": "@media-network/media-sn",
  "version": "1.0.0",
  "description": "For rapid prototyping Media CMS with prev/next/random navigator, FB integration and multiple themes",
  "main": "index.js",
  "repository": "git@github.com:media-network/media-sn.git",
  "author": "LongLH <i.me.mine@luchoanglong.com>",
  "license": "MIT",
  "scripts": {
    "start": "run.env nodemon ./src/site --exec babel-node --watch ./src/site --watch ./src/core --watch ./content/themes/default/override",
    "clear": "rm -rf ./data/build",
    "build": "npm run build:babel && npm run build:copy-ect",
    "build:babel": "babel ./src -d ./data/build --ignore=**/node_modules,src/themes/default/pages",
    "build:copy-ect": "copy ./src/**/*.ect ./data/build"
  },
  "dependencies": {
    "connect-slashes": "1.4.0",
    "cors": "2.8.5",
    "ect": "0.5.9",
    "express": "4.16.4",
    "find-free-port": "2.0.0",
    "fs-extra": "7.0.1",
    "http-proxy": "1.17.0",
    "kue": "0.11.6",
    "mongoose": "5.5.0",
    "morgan": "1.9.1",
    "ms": "2.1.1",
    "path-to-regexp": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/runtime": "7.4.3",
    "babel-plugin-module-resolver": "3.2.0",
    "concurrently": "4.1.0",
    "copy": "0.3.2",
    "glob": "7.1.3",
    "nodemon": "1.18.11",
    "run.env": "1.1.0",
    "webpack": "4.29.6",
    "webpack-assets-manifest": "3.1.1",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.6.2",
    "webpack-hot-middleware": "2.24.3"
  }
}
